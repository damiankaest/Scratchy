{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "scratchUpDev",
    "ConnectionTimeout": "00:00:15",
    "SocketTimeout": "00:00:30",
    "ServerSelectionTimeout": "00:00:15",
    "MaxConnectionPoolSize": 50,
    "MinConnectionPoolSize": 2,
    "MaxConnectionIdleTime": "00:05:00",
    "WaitQueueTimeout": "00:00:15",
    "EnableRetryWrites": true,
    "EnableRetryReads": true,
    "RetryAttempts": 2,
    "RetryDelay": "00:00:01",
    "Environment": "Development"
  }
}
